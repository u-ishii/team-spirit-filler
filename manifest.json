{
  "name": "team-sprit-filler",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://ap5.lightning.force.com/*"],
      "js": ["fill-remarks.js"],
      "all_frames": true
    }
  ]
}