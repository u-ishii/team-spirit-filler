{
  "name": "team-sprit-filler",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "fill-remarks.js"
      ],
      "all_frames": true
    }
  ]
}